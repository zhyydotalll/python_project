#用迭代器计算阶乘
def func_01(n):
    result = n
    for i in range(1,n):
        result *= i
    return  result
print(func_01(10))
#用递归函数计算阶乘
def func_02(n):
    if  n == 1 or n == 0:
        return 1
    else:
        result = n*func_02(n-1)
        return result
print(func_02(10))
#用lambda计算阶乘
result = lambda x : 1 if x ==0 or x ==1 else x*result(x-1)
print(result(10))
